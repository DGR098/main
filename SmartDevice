package poo.classes.ejercicios;

public class SmartDevice {

    String fabricant;
    String model;
    double size;
    int yearr;
    boolean plegable;
    int memoria;

    public SmartDevice(){
    }

    public SmartDevice(String fabricant, String model, double size, int yearr, boolean plegable, int memoria) {
        this.fabricant = fabricant;
        this.model = model;
        this.size = size;
        this.yearr = yearr;
        this.plegable = plegable;
        this.memoria = memoria;
    }

}
