package poo.classes.ejercicios;

public class SmartPhone extends SmartDevice {

    String color;
    public SmartPhone() {
        super();
    }
    public SmartPhone(String fabricant, String model, double size, int yearr, boolean plegable, int memoria, String color) {
        super(fabricant, model, size, yearr, plegable, memoria);
        this.color = color;
    }

    public SmartPhone(String apple, String iphone9, double v, int i, boolean b, int i1) {
    }

    @Override
    public String toString() {
        return "SmartPhone{" +
                "color='" + color + '\'' +
                ", fabricant='" + fabricant + '\'' +
                ", model='" + model + '\'' +
                ", size=" + size +
                ", yearr=" + yearr +
                ", plegable=" + plegable +
                ", memoria=" + memoria +
                '}';
    }
}
